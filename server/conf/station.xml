            <archives>
            <archive date="20110810" href="OpenAccessHour_20110810.mp3" hidden="0" playlistHref="OpenAccessHourPlaylist_20110810.txt" notes=""/><archive date="20110812" href="OpenAccessHour_20110812.mp3" hidden="0" playlistHref="OpenAccessHourPlaylist_20110812.txt" notes=""/><archive date="20110813" href="OpenAccessHour_20110813.mp3" hidden="0" playlistHref="OpenAccessHourPlaylist_20110813.txt" notes=""/><archive date="20110814" href="OpenAccessHour_20110814.mp3" hidden="0" playlistHref="OpenAccessHourPlaylist_20110814.txt" notes=""/></archives>
